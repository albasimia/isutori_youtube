{
  "name": "isutori_youtube",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Youtubeでイストリゲームをしよう！！！",
  "permissions":[
    "storage"
  ],
  "action": {
    "default_title": "イストリYoutube",
    "default_popup": "src/popup/popup.html"
  },
  "content_scripts":[
    {
      "matches":[
        "*://*.youtube.com/watch?*"
      ],
      "js":[
        "src/content.js"
      ]
    }
  ]
}